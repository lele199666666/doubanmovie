<!--logs.wxml-->
<view>
  <input class='input' bindinput='bindinputLele'></input>
  <button bindtap='searchLele' hover-class='btnClick'>查询</button>
   <text class="input-show">你要搜索：{{inputMovie}}</text>
   <import src="../template/movietpl.wxml"/>
   <block wx:for="{{movie}}">
    <template is="movie" data="{{item}}"/>
   </block>
   
</view>
<view id='outter' bindtap='outterTap'>
  outter view
  <view id='middle' catchtap='middleTap'>
    middle view
    <view id='inner' bindtap='innerTap'>inner view</view>
  </view>
  </view>


<view wx:for="{{[1,2,3,4,5,6,7,8,9]}}" wx:for-item="i">
  <view wx:for="{{[1,2,3,4,5,6,7,8,9]}}" wx:for-item="j" style="display:inline-block">
    <!-- <text>i={{i}}</text><text>j={{j}}</text> -->
   
    <view wx:if="{{j<=i}}">
       {{j}}*{{i}}={{i*j}}
    </view>
  </view>
</view>
<view class="container log-list">
  <block wx:for="{{logs}}" wx:for-item="log" wx:key="*this">
    <text class="log-item">{{index + 1}}. {{log}}</text>
  </block>
</view>
